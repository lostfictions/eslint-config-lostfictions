{
  "name": "eslint-config-lostfictions",
  "version": "3.8.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/lostfictions/eslint-config-lostfictions",
  "files": [
    "index.js",
    "react.js"
  ],
  "scripts": {
    "unused": "eslint-find-rules -un ./index.js",
    "deprecated": "eslint-find-rules -dn ./index.js",
    "test": "run-p -cl test:*",
    "test:configs": "eslint --color --max-warnings=0 '*.js'",
    "test:fixtures": "eslint --color --max-warnings=0 'test/**/*.{js,ts}'",
    "test:prettier": "prettier -l '**/*.{js,ts}'",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "prepublishOnly": "run-s test"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.41",
    "@types/react-dom": "^18.0.5",
    "eslint": "^8.17.0",
    "eslint-find-rules": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "eslint": "^8.14.0",
    "typescript": "^4.7.0"
  }
}
