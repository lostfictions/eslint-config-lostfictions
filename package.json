{
  "name": "eslint-config-lostfictions",
  "version": "3.1.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/lostfictions/eslint-config-lostfictions",
  "files": [
    "index.js",
    "react.js"
  ],
  "scripts": {
    "unused": "eslint-find-rules -un ./index.js",
    "deprecated": "eslint-find-rules -dn ./index.js",
    "test": "run-p -cl test:*",
    "test:configs": "eslint --color '*.js'",
    "test:fixtures": "eslint --color 'test/**/*.ts' --max-warnings=0",
    "test:prettier": "prettier -l '**/*.{js,ts}'",
    "prettier": "prettier --write '**/*.{js,ts}'"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.6",
    "eslint": "^8.5.0",
    "eslint-find-rules": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": "^8.5.0",
    "typescript": "^4.5.4"
  }
}
