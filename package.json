{
  "name": "eslint-config-lostfictions",
  "version": "7.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/lostfictions/eslint-config-lostfictions",
  "type": "module",
  "files": [
    "index.js",
    "index.d.ts",
    "react.js",
    "patch/index.js"
  ],
  "scripts": {
    "unused": "eslint-find-rules --flatConfig -un ./index.js",
    "deprecated": "ESLINT_CONFIG_PRETTIER_NO_DEPRECATED=true eslint-find-rules --flatConfig -dn ./index.js",
    "test": "run-p -cl test:*",
    "test:configs": "eslint --color --max-warnings=0 '*.{js,jsx,cjs,mjs,json,json5,jsonc}'",
    "test:fixtures": "TIMING=1 eslint --color --max-warnings=0 'test/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,json5,jsonc}'",
    "test:prettier": "prettier -l '**/*.{js,ts}'",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "prepublishOnly": "run-s test"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@eslint/json": "^0.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "globals": "^15.13.0",
    "typescript-eslint": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.68",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.16.0",
    "eslint-find-rules": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
